{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AllowedOrigins": "http://localhost:4200",
  "IsSwaggerEnabled": true,
  "DbSettings": {
    "ConnectionString": "Data Source=Posts.db"
  },
  "HackerNewsSettings": {
    "BaseUrl": "https://hacker-news.firebaseio.com/v0/",
    "TopStoriesUrl": "topstories.json",
    "GetByIdUrl": "item/{0}.json",
    "NewsCount": 20
  },
  "CacheSettings": {
    "ExpirationTime": "00:05"
  }
}
